                            li: a(href="/soca") Soca
                            li: a(href="/soul") Soul
                            li: a(href="/trap") Trap
                            li: a(href="/world") World
                            li: a(href="/yoruba") Yoruba
                    li: a.nav-link.active(href="/artistes") Artistes
                    li: a.nav-link(href="/singles") Singles
                    li: a.nav-link(href="/eps") EPs
            button#search-button.buttons.grid-center.material-icons search
        script(src="scripts/header.bundle.js")
        main
            section#artistes-container
                article
                    h1.letter &num;
                    ul.artistes
                        each val, key in artistes
                            if key.search(/(\d)/) === 0
                                li.name: a(href="artistes/" + key) #{val}
